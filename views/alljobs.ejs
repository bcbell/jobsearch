<%-include('./partials/header')%>

<body class="alljobs">
    



    <div class="alljobs">
        <% position.forEach(function(position){%>
      <div class="theresult">

       <a href="<%=position.url%>"><p><strong><h3><%= position.title%></h3></strong></a></p>
       <p><a href="<%=position.company_url%>"><%= position.company%></a></p> 
       <p><%= position.location%>(<%= position.type%>)</p>
      <%-position.description%>
       <p class="results"><a href='<%=position.how_to_apply%>'><button class="alljobs">Apply Here</button> </a></p>
      </div>
       <% })%>
    </div>


    
</body>